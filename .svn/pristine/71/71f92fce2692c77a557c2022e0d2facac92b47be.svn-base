<script>
    var banner=window.setInterval("bannerGo()",3000);
    num = 1;
    dem = 1;

    function bannerGo () {
        $('.banner_0'+dem).css('z-index', num);
        $(".banner_on").attr('src', '/Public/Plg/img/dian_off.png');
        $(".banner_on_0"+dem).attr('src', '/Public/Plg/img/dian_on.png');
        if (dem == 4) {
            dem = 1;
        };
        dem++;
        num++;
    }


    $(".banner-right").on('click', 'a', function() {
        var id = $(this).attr('data-banner-data');
        $(".sfq-on").hide();
        $(".sfq_"+id).show();
    });
</script>