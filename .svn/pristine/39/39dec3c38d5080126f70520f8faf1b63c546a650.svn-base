}

#title {
	text-align: left;
	margin-top: 15%;
	height: 200px;
	/*border: 1px solid red;*/
	color: white;
}

#title h1 {
	font-size: 40px;
	font-weight: 300;
}

#title h1 span{
	font-weight: 500;
	color: dodgerblue;
}

#title .element span{
	font-weight: 200;
	font-size: 18px;
}

#title a{
	text-align: center;
	border: 1px solid white;
	width: 200px;
	height: 60px;
